{
  "manifest_version": 3,
  "name": "Element Alert",
  "version": "1.0",
  "description": "Lấy element từ trang web và hiển thị alert",
  "permissions": [
      "activeTab"
  ],
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
  }
}
